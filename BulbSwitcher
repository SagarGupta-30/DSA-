#include <iostream>
#include <cmath>
using namespace std;

class Solution {
public:
    int bulbSwitch(int n) {
        return sqrt(n);  
    }
};

int main() {
    int n;
    cout << "Enter number of bulbs: ";
    cin >> n;

    Solution obj;
    int result = obj.bulbSwitch(n);

    cout << "Number of bulbs ON after " << n << " rounds: " << result << endl;

    return 0;
}
